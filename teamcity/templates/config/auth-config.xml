<?xml version="1.0" encoding="UTF-8"?>
<auth-config>
  <auth-modules>
    <auth-module type="Default">
      <property key="usersCanResetOwnPasswords">true</property>
      <property key="usersCanChangeOwnPasswords">true</property>
      <property key="freeRegistrationAllowed">true</property>
    </auth-module>
    <auth-module type="HTTP-Basic" />
  </auth-modules>
  <login-description />
  <guest-login allowed="true" guest-username="guest" />
</auth-config>

